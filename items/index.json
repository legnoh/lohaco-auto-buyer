{
  "01.\u98df\u54c1": "\u7c73,\u30d1\u30f3,\u5b85\u98df,\u30c6\u30a3\u30fc\u30d0\u30c3\u30b0(\u9ea6\u8336\u30fb\u51b7),\u30c6\u30a3\u30fc\u30d0\u30c3\u30b0(\u7d05\u8336\u30fb\u71b1),\u30a4\u30f3\u30b9\u30bf\u30f3\u30c8\u30b3\u30fc\u30d2\u30fc(\u71b1),\u91ce\u83dc\u30b8\u30e5\u30fc\u30b9,",
  "02.\u30ad\u30c3\u30c1\u30f3\u7528\u54c1": "\u98df\u6d17\u6a5f\u7528\u6d17\u5264,\u98df\u5668\u7528\u6d17\u5264,\u30b9\u30dd\u30f3\u30b8(\u98df\u5668\u7528),\u30b9\u30dd\u30f3\u30b8(\u6c34\u7b52\u7528),\u30b9\u30dd\u30f3\u30b8(\u30b7\u30f3\u30af\u7528),",
  "03.\u533b\u85ac\u54c1": "\u89e3\u71b1\u93ae\u75db\u85ac,\u80c3\u8178\u85ac,\u9f3b\u708e\u85ac,\u9154\u3044\u6b62\u3081\u85ac,\u53e3\u5185\u708e\u7528\u306e\u85ac,\u76ee\u85ac,\u51b7\u611f\u30b7\u30fc\u30c8,\u7d46\u5275\u818f(\u5927),\u7d46\u5275\u818f(\u4e2d),\u7d46\u5275\u818f(\u5c0f),\u30e1\u30ac\u30cd\u306e\u304f\u3082\u308a\u6b62\u3081,",
  "04.\u6587\u5177": "\u30e1\u30e2\u5e33,\u30dc\u30fc\u30eb\u30da\u30f3(\u9ed2),\u30de\u30b8\u30c3\u30af(\u9ed2),\u30de\u30b8\u30c3\u30af(\u8d64),\u7b46\u30da\u30f3,\u30b7\u30e3\u30fc\u30d7\u30da\u30f3\u30b7\u30eb,\u30bb\u30ed\u30c6\u30fc\u30d7,\u30ac\u30e0\u30c6\u30fc\u30d7,\u4e21\u9762\u30c6\u30fc\u30d7,\u30c6\u30fc\u30d7\u306e\u308a,\u77ac\u9593\u63a5\u7740\u5264,\u306f\u3055\u307f,\u30ab\u30c3\u30bf\u30fc(\u5c0f),\u30ab\u30c3\u30bf\u30fc(\u5927),\u5c01\u7b52,",
  "05.\u30dc\u30c7\u30a3\u30b1\u30a2\u30fb\u30b9\u30ad\u30f3\u30b1\u30a2\u7528\u54c1": "\u30b7\u30e3\u30f3\u30d7\u30fc,\u30c8\u30ea\u30fc\u30c8\u30e1\u30f3\u30c8,\u30dc\u30c7\u30a3\u30bd\u30fc\u30d7,\u6d17\u9854\u6599,\u5316\u7ca7\u6c34,\u4e73\u6db2,\u30ea\u30c3\u30d7\u30af\u30ea\u30fc\u30e0,\u30cf\u30f3\u30c9\u30bd\u30fc\u30d7,\u6b6f\u78e8\u304d\u7c89,\u5236\u6c57\u5264,\u6574\u9aea\u6599,\u30b7\u30a7\u30fc\u30d0\u30fc(\u66ff\u3048\u5203\u30fb\u5916),\u30b7\u30a7\u30fc\u30d0\u30fc(\u66ff\u3048\u5203\u30fb\u5185),\u30a8\u30c1\u30b1\u30c3\u30c8\u30ab\u30c3\u30bf\u30fc(\u66ff\u3048\u5203),\u30dc\u30c7\u30a3\u30bf\u30aa\u30eb,\u6b6f\u30d6\u30e9\u30b7,\u820c\u30d6\u30e9\u30b7,",
  "06.\u6d17\u6fef\u30fb\u98a8\u5442\u6383\u9664\u30fb\u30c8\u30a4\u30ec\u6383\u9664\u7528\u54c1": "\u8863\u985e\u7528\u6d17\u5264,\u30a8\u30ea\u305d\u3067\u7528\u6d17\u5264,\u8272\u79fb\u308a\u9632\u6b62\u30b7\u30fc\u30c8,\u98a8\u5442\u6d17\u5264,\u98a8\u5442\u30d6\u30e9\u30b7,\u98a8\u5442\u30b9\u30dd\u30f3\u30b8,\u305e\u3046\u304d\u3093,\u624b\u888b(\u6c34\u4ed5\u4e8b\u7528),\u30c8\u30a4\u30ec\u7528\u30a6\u30a7\u30c3\u30c8\u30c6\u30a3\u30c3\u30b7\u30e5,\u30c8\u30a4\u30ec\u7528\u30d6\u30e9\u30b7,\u30c8\u30a4\u30ec\u30b9\u30bf\u30f3\u30d7,",
  "07.\u7d19\u985e\u30fb\u6383\u9664\u7528\u54c1": "\u30b4\u30df\u888b,\u30c6\u30a3\u30c3\u30b7\u30e5(\u636e\u7f6e),\u30c6\u30a3\u30c3\u30b7\u30e5(\u30dd\u30b1\u30c3\u30c8),\u30c6\u30a3\u30c3\u30b7\u30e5(\u30a6\u30a7\u30c3\u30c8),\u30c6\u30a3\u30c3\u30b7\u30e5(\u6db2\u6676\u7528),\u8377\u9020\u308a\u3072\u3082,\u30b3\u30ed\u30b3\u30ed(\u7c98\u7740\u30af\u30ea\u30fc\u30ca\u30fc),\u30c8\u30a4\u30ec\u30c3\u30c8\u30da\u30fc\u30d1\u30fc,\u624b\u888b(\u4f5c\u696d\u7528),\u30af\u30ea\u30fc\u30cb\u30f3\u30b0\u30d6\u30e9\u30b7(\u96fb\u6c17\u7528\u54c1\u7528),",
  "08.\u305d\u306e\u4ed6": "\u624b\u888b(\u7c21\u6613\u4fdd\u8b77\u7528),\u901a\u6c17\u53e3\u7528\u30d5\u30a3\u30eb\u30bf\u30fc,"
}